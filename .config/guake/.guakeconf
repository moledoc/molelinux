[general]
abbreviate-tab-names=false
compat-delete='delete-sequence'
display-n=0
gtk-prefer-dark-theme=true
gtk-theme-name='Mint-Y-Dark-Orange'
history-size=5000
max-tab-name-length=100
mouse-display=false
open-tab-cwd=true
prompt-on-close-tab=2
prompt-on-quit=true
quick-open-command-line='nvim %(file_path)s'
quick-open-enable=true
quick-open-in-current-terminal=true
restore-tabs-notify=false
restore-tabs-startup=false
save-tabs-when-changed=false
scroll-keystroke=true
start-at-login=true
tab-close-buttons=true
tab-ontop=false
use-default-font=false
use-popup=true
use-scrollbar=true
use-trayicon=true
window-halignment=0
window-height=37
window-losefocus=true
window-refocus=false
window-tabbar=true
window-valignment=0
window-vertical-displacement=1
window-width=100
workspace-specific-tab-sets=true

[keybindings/global]
show-hide='F12'

[keybindings/local]
close-tab='<Primary><Shift>q'
close-terminal='<Primary>q'
decrease-height='<Primary><Shift>Up'
decrease-transparency='<Primary><Alt>Down'
focus-terminal-down='<Primary>j'
focus-terminal-left='<Primary>h'
focus-terminal-right='<Primary>l'
focus-terminal-up='<Primary>k'
increase-height='<Primary><Shift>Down'
increase-transparency='<Primary><Alt>Up'
move-terminal-split-down='<Primary>Down'
move-terminal-split-left='<Primary>Left'
move-terminal-split-right='<Primary>Right'
move-terminal-split-up='<Primary>Up'
new-tab-home='<Primary><Shift><Alt>t'
split-tab-horizontal='<Primary>less'
split-tab-vertical='<Primary>greater'
switch-tab-last='<Primary>0'
switch-tab1='<Primary>1'
switch-tab10='disabled'
switch-tab2='<Primary>2'
switch-tab3='<Primary>3'
switch-tab4='<Primary>4'
switch-tab5='<Primary>5'
switch-tab6='<Primary>6'
switch-tab7='<Primary>7'
switch-tab8='<Primary>8'
switch-tab9='<Primary>9'
toggle-transparency='disabled'

[style/background]
transparency=99

[style/font]
allow-bold=true
palette='#282828282828:#cccc24241d1d:#989897971a19:#d7d799992120:#454585858888:#b1b162618685:#68689d9d6a6a:#a8a899998484:#929283837474:#fbfb49493434:#b8b8bbbb2626:#fafabdbc2f2f:#8383a5a59898:#d3d386869b9b:#8e8ec0c07c7b:#ebebdbdbb2b2:#fafabdbc2f2f:#282828282828'
palette-name='Custom'
style='Hermit Medium 10'
